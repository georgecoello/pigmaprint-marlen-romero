{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport AppHeader from '@/components/Header.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // Modo administrador (en producción esto vendría de autenticación)\n      isAdmin: true,\n      // Cambiar a false para modo cliente normal\n\n      // Muestras de productos existentes\n      sampleProducts: [{\n        id: 'p1',\n        name: 'Camiseta Personalizada',\n        desc: 'Diseño de logo empresarial en algodón 100%',\n        image: '/images/sample-tshirt.jpg' // Ruta de ejemplo\n      }, {\n        id: 'p2',\n        name: 'Taza Cerámica',\n        desc: 'Sublimación full color con diseño único',\n        image: '/images/sample-mug.jpg' // Ruta de ejemplo\n      }, {\n        id: 'p3',\n        name: 'Termo Acero',\n        desc: 'Impresión DTF resistente para uso diario',\n        image: '/images/sample-bottle.jpg' // Ruta de ejemplo\n      }],\n      // Nueva muestra para administrador\n      newSample: {\n        name: '',\n        desc: '',\n        image: null\n      },\n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click();\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0];\n      console.log('Archivo subido:', this.uploadedFile);\n    },\n    // Funciones para administrador - Muestras de productos\n    handleNewSampleUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n\n      // Crear URL temporal para previsualización\n      this.newSample.image = URL.createObjectURL(file);\n      this.newSample.imageFile = file;\n    },\n    addNewSample() {\n      if (!this.newSample.name || !this.newSample.desc || !this.newSample.image) {\n        alert('Por favor completa todos los campos y selecciona una imagen.');\n        return;\n      }\n\n      // En una aplicación real, aquí subirías la imagen a un servidor\n      // y obtendrías una URL permanente\n      const newProduct = {\n        id: 'p' + (this.sampleProducts.length + 1),\n        name: this.newSample.name,\n        desc: this.newSample.desc,\n        image: this.newSample.image // En producción sería una URL del servidor\n      };\n      this.sampleProducts.unshift(newProduct);\n\n      // Limpiar formulario\n      this.newSample = {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null\n      };\n      alert('Muestra agregada exitosamente a la galería!');\n    },\n    removeSample(productId) {\n      if (confirm('¿Estás seguro de que quieres eliminar esta muestra?')) {\n        const index = this.sampleProducts.findIndex(p => p.id === productId);\n        if (index !== -1) {\n          this.sampleProducts.splice(index, 1);\n        }\n      }\n    },\n    // Diseño personalizado\n    handleDesignUpload(event) {\n      const file = event.target.files && event.target.files[0];\n      if (!file) return;\n      if (this.designPreview) {\n        try {\n          URL.revokeObjectURL(this.designPreview);\n        } catch (e) {\n          console.warn('Could not revoke object URL (design):', e);\n        }\n      }\n      this.designFile = file;\n      this.designPreview = URL.createObjectURL(file);\n      console.log('Diseño cargado:', file);\n    },\n    submitDesign() {\n      if (!this.designFile) {\n        alert('Por favor sube una imagen para tu diseño.');\n        return;\n      }\n      if (!this.designProductType) {\n        alert('Selecciona el producto donde quieres imprimir el diseño.');\n        return;\n      }\n      this.cartCount += 1;\n      console.log('Diseño enviado:', {\n        file: this.designFile,\n        product: this.designProductType,\n        notes: this.designNotes\n      });\n      this.designFile = null;\n      try {\n        URL.revokeObjectURL(this.designPreview);\n      } catch (e) {\n        console.warn('Could not revoke object URL (cleanup):', e);\n      }\n      this.designPreview = null;\n      this.designProductType = '';\n      this.designNotes = '';\n      this.$router.push('/cart');\n    },\n    // Contacto\n    submitContact() {\n      console.log('Contacto enviado:', this.contact);\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contactaremos pronto.');\n      this.contact = {\n        name: '',\n        email: '',\n        message: ''\n      };\n    },\n    addToCart() {\n      this.cartCount += this.quantity;\n      this.$router.push('/cart');\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","name","components","data","selectedProduct","quantity","instructions","uploadedFile","cartCount","isAdmin","sampleProducts","id","desc","image","newSample","designFile","designPreview","designProductType","designNotes","contact","email","message","methods","triggerFileInput","document","getElementById","click","handleFileUpload","event","target","files","console","log","handleNewSampleUpload","file","URL","createObjectURL","imageFile","addNewSample","alert","newProduct","length","unshift","removeSample","productId","confirm","index","findIndex","p","splice","handleDesignUpload","revokeObjectURL","e","warn","submitDesign","product","notes","$router","push","submitContact","addToCart"],"sources":["/home/jorge/Escritorio/Jorge-PC/Marlen/pigmaprint/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen bg-gradient-to-br from-gray-50 to-gray-100 dark:from-gray-900 dark:to-gray-800\">\n    <AppHeader :cart-count=\"cartCount\" />\n    <main class=\"flex-grow container mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <div class=\"max-w-3xl mx-auto\">\n        <!-- Texto de bienvenida mejorado -->\n        <section class=\"mb-12 text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8 transform transition-all duration-300 hover:shadow-xl\">\n          <h1 class=\"text-4xl sm:text-5xl font-extrabold tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">\n            ¡Bienvenido a Pigmaprint!\n          </h1>\n          <p class=\"mt-6 text-lg text-gray-600 dark:text-gray-300 leading-relaxed\">\n            Donde tu imaginación se convierte en realidad. Creemos que cada idea merece ser única, por eso personalizamos todo lo que puedas imaginar: desde prendas hasta tazas, termos, vasos, llaveros y mucho más.\n          </p>\n          <div class=\"mt-8 bg-gradient-to-r from-purple-50 to-blue-50 dark:from-gray-700 dark:to-gray-600 rounded-xl p-6\">\n            <h3 class=\"text-xl font-bold text-gray-800 dark:text-white mb-4\">Tecnologías de impresión:</h3>\n            <ul class=\"grid grid-cols-1 md:grid-cols-3 gap-4 text-left\">\n              <li class=\"flex items-start\">\n                <span class=\"text-purple-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>UVDTF</strong> — acabados duraderos con relieve</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-blue-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>DTF textil</strong> — ideal para cualquier color o textura</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-green-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>Sublimación</strong> — resultados vibrantes</span>\n              </li>\n            </ul>\n          </div>\n          <p class=\"mt-8 text-base text-gray-600 dark:text-gray-300 italic border-l-4 border-purple-500 pl-4 py-2\">\n            Nuestra misión es empoderarte para que transformes tus ideas en piezas únicas, llenas de estilo, emoción y personalidad.\n          </p>\n        </section>\n        \n        <!-- Muestras de Productos - Ahora para admin subir productos creados -->\n        <section class=\"mt-16\">\n          <div class=\"text-center mb-10\">\n            <h3 class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent\">Nuestros Trabajos Realizados</h3>\n            <p class=\"mt-2 text-gray-600 dark:text-gray-400 max-w-2xl mx-auto\">Galería de productos personalizados que hemos creado para nuestros clientes.</p>\n            \n            <!-- Panel de administrador para subir nuevas muestras -->\n            <div v-if=\"isAdmin\" class=\"mt-6 bg-yellow-50 dark:bg-yellow-900/20 border border-yellow-200 dark:border-yellow-700 rounded-xl p-6 max-w-md mx-auto\">\n              <h4 class=\"text-lg font-semibold text-yellow-800 dark:text-yellow-200 mb-4\">Panel de Administrador</h4>\n              <div class=\"space-y-4\">\n                <div>\n                  <label class=\"block text-sm font-medium text-yellow-700 dark:text-yellow-300 mb-2\">Nombre del producto</label>\n                  <input \n                    v-model=\"newSample.name\" \n                    type=\"text\" \n                    class=\"w-full rounded-lg p-3 border border-yellow-300 dark:border-yellow-600 bg-white dark:bg-gray-800 text-gray-800 dark:text-white\"\n                    placeholder=\"Ej: Camiseta personalizada\"\n                  />\n                </div>\n                <div>\n                  <label class=\"block text-sm font-medium text-yellow-700 dark:text-yellow-300 mb-2\">Descripción</label>\n                  <input \n                    v-model=\"newSample.desc\" \n                    type=\"text\" \n                    class=\"w-full rounded-lg p-3 border border-yellow-300 dark:border-yellow-600 bg-white dark:bg-gray-800 text-gray-800 dark:text-white\"\n                    placeholder=\"Ej: Diseño de logo empresarial\"\n                  />\n                </div>\n                <div>\n                  <label class=\"block text-sm font-medium text-yellow-700 dark:text-yellow-300 mb-2\">Imagen del producto</label>\n                  <input \n                    type=\"file\" \n                    accept=\"image/*\" \n                    @change=\"handleNewSampleUpload\"\n                    class=\"w-full text-sm text-yellow-700 dark:text-yellow-300 file:mr-4 file:py-2 file:px-4 file:rounded-lg file:border-0 file:text-sm file:font-semibold file:bg-yellow-500 file:text-white hover:file:bg-yellow-600\"\n                  />\n                </div>\n                <button \n                  @click=\"addNewSample\" \n                  class=\"w-full py-3 rounded-lg bg-yellow-500 hover:bg-yellow-600 text-white font-medium transition-colors duration-300\"\n                >\n                  Agregar a Galería\n                </button>\n              </div>\n            </div>\n          </div>\n          \n          <!-- Galería de muestras existentes -->\n          <div class=\"mt-6 grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8\">\n            <div \n              v-for=\"product in sampleProducts\" \n              :key=\"product.id\" \n              class=\"border border-gray-200 dark:border-gray-700 rounded-xl p-6 bg-white dark:bg-gray-800 shadow-md hover:shadow-lg transition-all duration-300 transform hover:-translate-y-1\"\n            >\n              <div class=\"relative\">\n                <img \n                  :src=\"product.image\" \n                  :alt=\"product.name\" \n                  class=\"w-full h-48 object-cover rounded-lg mb-4\"\n                />\n                <span \n                  v-if=\"isAdmin\"\n                  @click=\"removeSample(product.id)\"\n                  class=\"absolute top-2 right-2 bg-red-500 text-white p-1 rounded-full cursor-pointer hover:bg-red-600 transition-colors\"\n                >\n                  <span class=\"material-symbols-outlined text-sm\">delete</span>\n                </span>\n              </div>\n              <h4 class=\"font-bold text-lg text-gray-800 dark:text-white\">{{ product.name }}</h4>\n              <p class=\"mt-2 text-sm text-gray-600 dark:text-gray-400\">{{ product.desc }}</p>\n            </div>\n          </div>\n          \n          <!-- Mensaje cuando no hay muestras -->\n          <div v-if=\"sampleProducts.length === 0\" class=\"text-center py-12\">\n            <div class=\"text-6xl text-gray-300 dark:text-gray-600 mb-4\">\n              <span class=\"material-symbols-outlined\">photo_library</span>\n            </div>\n            <p class=\"text-gray-500 dark:text-gray-400\">Aún no hay muestras en la galería.</p>\n            <p v-if=\"isAdmin\" class=\"text-sm text-gray-400 dark:text-gray-500 mt-2\">Usa el panel de administrador para agregar la primera muestra.</p>\n          </div>\n        </section>\n\n        <!-- Carga tu Diseño mejorado -->\n        <section class=\"mt-16\">\n          <div class=\"text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <h2 class=\"text-4xl font-black tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">Carga tu Diseño</h2>\n            <p class=\"mt-4 text-lg text-gray-600 dark:text-gray-300\">Sube tu diseño para personalizar tus productos. Aceptamos archivos PNG, JPG, SVG y PDF.</p>\n          </div>\n\n          <!-- Área de carga de archivos mejorada -->\n          <div class=\"mt-8\">\n            <h2 class=\"block text-lg font-semibold pb-3 text-gray-800 dark:text-white\">Subir Archivo</h2>\n            <div \n              @click=\"triggerFileInput\"\n              class=\"flex flex-col items-center justify-center border-2 border-dashed border-gray-300 dark:border-gray-600 rounded-2xl h-56 cursor-pointer transition-all duration-300 bg-gradient-to-br from-gray-50 to-white dark:from-gray-800 dark:to-gray-700 hover:border-purple-400 hover:shadow-md\"\n            >\n              <input \n                id=\"fileInput\"\n                type=\"file\"\n                class=\"hidden\"\n                @change=\"handleFileUpload\"\n                accept=\".png, .jpg, .jpeg, .svg, .pdf\"\n              />\n              <div class=\"text-6xl text-purple-400 mb-4\">\n                <span class=\"material-symbols-outlined\">cloud_upload</span>\n              </div>\n              <p class=\"text-lg text-gray-600 dark:text-gray-400 font-medium\">Haz clic para seleccionar un archivo</p>\n              <p class=\"mt-2 text-sm text-gray-500 dark:text-gray-500\">o arrástralo aquí</p>\n              <p v-if=\"uploadedFile\" class=\"mt-4 px-4 py-2 bg-green-100 dark:bg-green-900 text-green-800 dark:text-green-200 rounded-lg text-sm font-medium\">\n                Archivo seleccionado: {{ uploadedFile.name }}\n              </p>\n            </div>\n          </div>\n        </section>\n\n        <!-- Sección de contacto mejorada con redes sociales -->\n        <section class=\"mt-16 pb-12\">\n          <div class=\"bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <h3 class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent mb-2\">Contacto</h3>\n            <p class=\"text-gray-600 dark:text-gray-400 mb-6\">Si tienes preguntas o quieres un presupuesto personalizado, escríbenos.</p>\n            \n            <!-- Redes Sociales -->\n            <div class=\"mb-8\">\n              <h4 class=\"text-lg font-semibold text-gray-800 dark:text-white mb-4\">Síguenos en redes sociales</h4>\n              <div class=\"flex space-x-4\">\n                <a \n                  href=\"https://facebook.com/pigmaprint\" \n                  target=\"_blank\"\n                  class=\"flex items-center justify-center w-12 h-12 bg-blue-600 text-white rounded-full hover:bg-blue-700 transition-colors duration-300\"\n                  title=\"Síguenos en Facebook\"\n                >\n                  <span class=\"material-symbols-outlined\">thumb_up</span>\n                </a>\n                <a \n                  href=\"https://instagram.com/pigmaprint\" \n                  target=\"_blank\"\n                  class=\"flex items-center justify-center w-12 h-12 bg-gradient-to-r from-purple-600 to-pink-500 text-white rounded-full hover:from-purple-700 hover:to-pink-600 transition-all duration-300\"\n                  title=\"Síguenos en Instagram\"\n                >\n                  <span class=\"material-symbols-outlined\">photo_camera</span>\n                </a>\n              </div>\n              <p class=\"mt-3 text-sm text-gray-500 dark:text-gray-400\">\n                Mantente al día con nuestros últimos trabajos y promociones especiales.\n              </p>\n            </div>\n            \n            <form @submit.prevent=\"submitContact\" class=\"space-y-6 max-w-xl\">\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Nombre</label>\n                <input \n                  v-model=\"contact.name\" \n                  type=\"text\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required \n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Email</label>\n                <input \n                  v-model=\"contact.email\" \n                  type=\"email\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required \n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Mensaje</label>\n                <textarea \n                  v-model=\"contact.message\" \n                  rows=\"4\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required\n                ></textarea>\n              </div>\n              <div class=\"flex justify-end\">\n                <button \n                  type=\"submit\" \n                  class=\"px-8 py-3 rounded-xl bg-gradient-to-r from-purple-500 to-blue-500 text-white font-medium shadow-md hover:shadow-lg transition-all duration-300 hover:from-purple-600 hover:to-blue-600 transform hover:-translate-y-0.5\"\n                >\n                  Enviar Mensaje\n                </button>\n              </div>\n            </form>\n          </div>\n        </section>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppHeader from '@/components/Header.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // Modo administrador (en producción esto vendría de autenticación)\n      isAdmin: true, // Cambiar a false para modo cliente normal\n      \n      // Muestras de productos existentes\n      sampleProducts: [\n        { \n          id: 'p1', \n          name: 'Camiseta Personalizada', \n          desc: 'Diseño de logo empresarial en algodón 100%',\n          image: '/images/sample-tshirt.jpg' // Ruta de ejemplo\n        },\n        { \n          id: 'p2', \n          name: 'Taza Cerámica', \n          desc: 'Sublimación full color con diseño único',\n          image: '/images/sample-mug.jpg' // Ruta de ejemplo\n        },\n        { \n          id: 'p3', \n          name: 'Termo Acero', \n          desc: 'Impresión DTF resistente para uso diario',\n          image: '/images/sample-bottle.jpg' // Ruta de ejemplo\n        }\n      ],\n      \n      // Nueva muestra para administrador\n      newSample: {\n        name: '',\n        desc: '',\n        image: null\n      },\n      \n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    }\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click()\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0]\n      console.log('Archivo subido:', this.uploadedFile)\n    },\n    \n    // Funciones para administrador - Muestras de productos\n    handleNewSampleUpload(event) {\n      const file = event.target.files[0]\n      if (!file) return\n      \n      // Crear URL temporal para previsualización\n      this.newSample.image = URL.createObjectURL(file)\n      this.newSample.imageFile = file\n    },\n    \n    addNewSample() {\n      if (!this.newSample.name || !this.newSample.desc || !this.newSample.image) {\n        alert('Por favor completa todos los campos y selecciona una imagen.')\n        return\n      }\n      \n      // En una aplicación real, aquí subirías la imagen a un servidor\n      // y obtendrías una URL permanente\n      const newProduct = {\n        id: 'p' + (this.sampleProducts.length + 1),\n        name: this.newSample.name,\n        desc: this.newSample.desc,\n        image: this.newSample.image // En producción sería una URL del servidor\n      }\n      \n      this.sampleProducts.unshift(newProduct)\n      \n      // Limpiar formulario\n      this.newSample = {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null\n      }\n      \n      alert('Muestra agregada exitosamente a la galería!')\n    },\n    \n    removeSample(productId) {\n      if (confirm('¿Estás seguro de que quieres eliminar esta muestra?')) {\n        const index = this.sampleProducts.findIndex(p => p.id === productId)\n        if (index !== -1) {\n          this.sampleProducts.splice(index, 1)\n        }\n      }\n    },\n    \n    // Diseño personalizado\n    handleDesignUpload(event) {\n      const file = event.target.files && event.target.files[0]\n      if (!file) return\n      if (this.designPreview) {\n        try { URL.revokeObjectURL(this.designPreview) } catch (e) { console.warn('Could not revoke object URL (design):', e) }\n      }\n      this.designFile = file\n      this.designPreview = URL.createObjectURL(file)\n      console.log('Diseño cargado:', file)\n    },\n    \n    submitDesign() {\n      if (!this.designFile) {\n        alert('Por favor sube una imagen para tu diseño.')\n        return\n      }\n      if (!this.designProductType) {\n        alert('Selecciona el producto donde quieres imprimir el diseño.')\n        return\n      }\n      this.cartCount += 1\n      console.log('Diseño enviado:', { file: this.designFile, product: this.designProductType, notes: this.designNotes })\n      this.designFile = null\n      try { URL.revokeObjectURL(this.designPreview) } catch (e) { console.warn('Could not revoke object URL (cleanup):', e) }\n      this.designPreview = null\n      this.designProductTyp