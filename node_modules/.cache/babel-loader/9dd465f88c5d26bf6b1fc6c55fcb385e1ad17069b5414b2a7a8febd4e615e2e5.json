{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport AppHeader from '@/components/Header.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // muestras\n      sampleProducts: [{\n        id: 'p1',\n        name: 'Camisa - Muestra',\n        desc: 'Sube una foto de una camisa impresa'\n      }, {\n        id: 'p2',\n        name: 'Taza - Muestra',\n        desc: 'Sube una foto de una taza impresa'\n      }, {\n        id: 'p3',\n        name: 'Termo - Muestra',\n        desc: 'Sube una foto de un termo impreso'\n      }],\n      samplesPreview: {},\n      samplesFiles: {},\n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click();\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0];\n      // Aquí puedes procesar el archivo subido\n      console.log('Archivo subido:', this.uploadedFile);\n    },\n    // Muestras (cards)\n    triggerSampleInput(id) {\n      const el = document.getElementById(`sampleFile_${id}`);\n      if (el) el.click();\n    },\n    handleSampleUpload(event, id) {\n      const file = event.target.files && event.target.files[0];\n      if (!file) return;\n      // liberar preview anterior si existiera\n      if (this.samplesPreview[id]) {\n        try {\n          URL.revokeObjectURL(this.samplesPreview[id]);\n        } catch (e) {\n          console.warn('Could not revoke object URL (sample):', e);\n        }\n      }\n      this.samplesFiles[id] = file;\n      this.samplesPreview[id] = URL.createObjectURL(file);\n      console.log('Muestra subida para', id, file);\n    },\n    // Diseño personalizado\n    handleDesignUpload(event) {\n      const file = event.target.files && event.target.files[0];\n      if (!file) return;\n      if (this.designPreview) {\n        try {\n          URL.revokeObjectURL(this.designPreview);\n        } catch (e) {\n          console.warn('Could not revoke object URL (design):', e);\n        }\n      }\n      this.designFile = file;\n      this.designPreview = URL.createObjectURL(file);\n      console.log('Diseño cargado:', file);\n    },\n    submitDesign() {\n      if (!this.designFile) {\n        alert('Por favor sube una imagen para tu diseño.');\n        return;\n      }\n      if (!this.designProductType) {\n        alert('Selecciona el producto donde quieres imprimir el diseño.');\n        return;\n      }\n      // En una app real enviaríamos a backend o al carrito. Aquí simulamos agregar 1 al carrito y redirigir.\n      this.cartCount += 1;\n      console.log('Diseño enviado:', {\n        file: this.designFile,\n        product: this.designProductType,\n        notes: this.designNotes\n      });\n      // Limpieza simple\n      this.designFile = null;\n      try {\n        URL.revokeObjectURL(this.designPreview);\n      } catch (e) {\n        console.warn('Could not revoke object URL (cleanup):', e);\n      }\n      this.designPreview = null;\n      this.designProductType = '';\n      this.designNotes = '';\n      this.$router.push('/cart');\n    },\n    // Contacto\n    submitContact() {\n      // Aquí enviaríamos a un endpoint. Por ahora solo log y notificación.\n      console.log('Contacto enviado:', this.contact);\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contactaremos pronto.');\n      this.contact = {\n        name: '',\n 