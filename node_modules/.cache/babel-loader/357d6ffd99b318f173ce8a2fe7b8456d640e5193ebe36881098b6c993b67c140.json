{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport AppHeader from '@/components/Header.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // muestras\n      sampleProducts: [{\n        id: 'p1',\n        name: 'Camisa - Muestra',\n        desc: 'Sube una foto de una camisa impresa'\n      }, {\n        id: 'p2',\n        name: 'Taza - Muestra',\n        desc: 'Sube una foto de una taza impresa'\n      }, {\n        id: 'p3',\n        name: 'Termo - Muestra',\n        desc: 'Sube una foto de un termo impreso'\n      }],\n      samplesPreview: {},\n      samplesFiles: {},\n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click();\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0];\n      // Aquí puedes procesar el archivo subido\n      console.log('Archivo subido:', this.uploadedFile);\n    },\n    // Muestras (cards)\n    triggerSampleInput(id) {\n      const el = document.getElementById(`sampleFile_${id}`);\n      if (el) el.click();\n    },\n    handleSampleUpload(event, id) {\n      const file = event.target.files && event.target.files[0];\n      if (!file) return;\n      // liberar preview anterior si existiera\n      if (this.samplesPreview[id]) {\n        try {\n          URL.revokeObjectURL(this.samplesPreview[id]);\n        } catch (e) {\n          console.warn('Could not revoke object URL (sample):', e);\n        }\n      }\n      this.samplesFiles[id] = file;\n      this.samplesPreview[id] = URL.createObjectURL(file);\n      console.log('Muestra subida para', id, file);\n    },\n    // Diseño personalizado\n    handleDesignUpload(event) {\n      const file = event.target.files && event.target.files[0];\n      if (!file) return;\n      if (this.designPreview) {\n        try {\n          URL.revokeObjectURL(this.designPreview);\n        } catch (e) {\n          console.warn('Could not revoke object URL (design):', e);\n        }\n      }\n      this.designFile = file;\n      this.designPreview = URL.createObjectURL(file);\n      console.log('Diseño cargado:', file);\n    },\n    submitDesign() {\n      if (!this.designFile) {\n        alert('Por favor sube una imagen para tu diseño.');\n        return;\n      }\n      if (!this.designProductType) {\n        alert('Selecciona el producto donde quieres imprimir el diseño.');\n        return;\n      }\n      // En una app real enviaríamos a backend o al carrito. Aquí simulamos agregar 1 al carrito y redirigir.\n      this.cartCount += 1;\n      console.log('Diseño enviado:', {\n        file: this.designFile,\n        product: this.designProductType,\n        notes: this.designNotes\n      });\n      // Limpieza simple\n      this.designFile = null;\n      try {\n        URL.revokeObjectURL(this.designPreview);\n      } catch (e) {\n        console.warn('Could not revoke object URL (cleanup):', e);\n      }\n      this.designPreview = null;\n      this.designProductType = '';\n      this.designNotes = '';\n      this.$router.push('/cart');\n    },\n    // Contacto\n    submitContact() {\n      // Aquí enviaríamos a un endpoint. Por ahora solo log y notificación.\n      console.log('Contacto enviado:', this.contact);\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contactaremos pronto.');\n      this.contact = {\n        name: '',\n        email: '',\n        message: ''\n      };\n    },\n    addToCart() {\n      // Lógica para agregar al carrito\n      this.cartCount += this.quantity;\n\n      // Redirigir al carrito\n      this.$router.push('/cart');\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","name","components","data","selectedProduct","quantity","instructions","uploadedFile","cartCount","sampleProducts","id","desc","samplesPreview","samplesFiles","designFile","designPreview","designProductType","designNotes","contact","email","message","methods","triggerFileInput","document","getElementById","click","handleFileUpload","event","target","files","console","log","triggerSampleInput","el","handleSampleUpload","file","URL","revokeObjectURL","e","warn","createObjectURL","handleDesignUpload","submitDesign","alert","product","notes","$router","push","submitContact","addToCart"],"sources":["/home/jorge/Escritorio/Jorge-PC/Marlen/pigmaprint/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen bg-gradient-to-br from-gray-50 to-gray-100 dark:from-gray-900 dark:to-gray-800\">\n    <AppHeader :cart-count=\"cartCount\" />\n    <main class=\"flex-grow container mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <div class=\"max-w-3xl mx-auto\">\n        <!-- Texto de bienvenida mejorado -->\n        <section class=\"mb-12 text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8 transform transition-all duration-300 hover:shadow-xl\">\n          <h1 class=\"text-4xl sm:text-5xl font-extrabold tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">\n            ¡Bienvenido a Pigmaprint!\n          </h1>\n          <p class=\"mt-6 text-lg text-gray-600 dark:text-gray-300 leading-relaxed\">\n            Donde tu imaginación se convierte en realidad. Creemos que cada idea merece ser única, por eso personalizamos todo lo que puedas imaginar: desde prendas hasta tazas, termos, vasos, llaveros y mucho más.\n          </p>\n          <div class=\"mt-8 bg-gradient-to-r from-purple-50 to-blue-50 dark:from-gray-700 dark:to-gray-600 rounded-xl p-6\">\n            <h3 class=\"text-xl font-bold text-gray-800 dark:text-white mb-4\">Tecnologías de impresión:</h3>\n            <ul class=\"grid grid-cols-1 md:grid-cols-3 gap-4 text-left\">\n              <li class=\"flex items-start\">\n                <span class=\"text-purple-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>UVDTF</strong> — acabados duraderos con relieve</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-blue-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>DTF textil</strong> — ideal para cualquier color o textura</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-green-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>Sublimación</strong> — resultados vibrantes</span>\n              </li>\n            </ul>\n          </div>\n          <p class=\"mt-8 text-base text-gray-600 dark:text-gray-300 italic border-l-4 border-purple-500 pl-4 py-2\">\n            Nuestra misión es empoderarte para que transformes tus ideas en piezas únicas, llenas de estilo, emoción y personalidad.\n          </p>\n        </section>\n        \n        <!-- Muestras de Productos mejorado -->\n        <section class=\"mt-16\">\n          <div class=\"text-center mb-10\">\n            <h3 class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent\">Muestras de Productos</h3>\n            <p class=\"mt-2 text-gray-600 dark:text-gray-400 max-w-2xl mx-auto\">Sube una foto de un producto ya realizado para que podamos ver la calidad y tonos de impresión.</p>\n          </div>\n          <div class=\"mt-6 grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8\">\n            <div \n              v-for=\"product in sampleProducts\" \n              :key=\"product.id\" \n              class=\"border border-gray-200 dark:border-gray-700 rounded-xl p-6 bg-white dark:bg-gray-800 shadow-md hover:shadow-lg transition-all duration-300 transform hover:-translate-y-1\"\n            >\n              <h4 class=\"font-bold text-lg text-gray-800 dark:text-white\">{{ product.name }}</h4>\n              <p class=\"mt-2 text-sm text-gray-600 dark:text-gray-400\">{{ product.desc }}</p>\n              <div class=\"mt-6\">\n                <div v-if=\"samplesPreview[product.id]\" class=\"mb-4 overflow-hidden rounded-lg\">\n                  <img \n                    :src=\"samplesPreview[product.id]\" \n                    alt=\"preview\" \n                    class=\"w-full h-40 object-cover rounded-lg transition-transform duration-500 hover:scale-105\" \n                  />\n                </div>\n                <input \n                  :id=\"`sampleFile_${product.id}`\" \n                  type=\"file\" \n                  accept=\"image/*\" \n                  class=\"hidden\" \n                  @change=\"handleSampleUpload($event, product.id)\" \n                />\n                <button \n                  @click.prevent=\"triggerSampleInput(product.id)\" \n                  class=\"w-full py-3 rounded-xl bg-gradient-to-r from-purple-500 to-blue-500 text-white font-medium shadow-md hover:shadow-lg transition-all duration-300 hover:from-purple-600 hover:to-blue-600\"\n                >\n                  Subir Muestra\n                </button>\n              </div>\n            </div>\n          </div>\n        </section>\n\n        <!-- Carga tu Diseño mejorado -->\n        <section class=\"mt-16\">\n          <div class=\"text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <h2 class=\"text-4xl font-black tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">Carga tu Diseño</h2>\n            <p class=\"mt-4 text-lg text-gray-600 dark:text-gray-300\">Sube tu diseño para personalizar tus productos. Aceptamos archivos PNG, JPG, SVG y PDF.</p>\n          </div>\n\n          <!-- Área de carga de archivos mejorada -->\n          <div class=\"mt-8\">\n            <h2 class=\"block text-lg font-semibold pb-3 text-gray-800 dark:text-white\">Subir Archivo</h2>\n            <div \n              @click=\"triggerFileInput\"\n              class=\"flex flex-col items-center justify-center border-2 border-dashed border-gray-300 dark:border-gray-600 rounded-2xl h-56 cursor-pointer transition-all duration-300 bg-gradient-to-br from-gray-50 to-white dark:from-gray-800 dark:to-gray-700 hover:border-purple-400 hover:shadow-md\"\n            >\n              <input \n                id=\"fileInput\"\n                type=\"file\"\n                class=\"hidden\"\n                @change=\"handleFileUpload\"\n                accept=\".png, .jpg, .jpeg, .svg, .pdf\"\n              />\n              <div class=\"text-6xl text-purple-400 mb-4\">\n                <span class=\"material-symbols-outlined\">cloud_upload</span>\n              </div>\n              <p class=\"text-lg text-gray-600 dark:text-gray-400 font-medium\">Haz clic para seleccionar un archivo</p>\n              <p class=\"mt-2 text-sm text-gray-500 dark:text-gray-500\">o arrástralo aquí</p>\n              <p v-if=\"uploadedFile\" class=\"mt-4 px-4 py-2 bg-green-100 dark:bg-green-900 text-green-800 dark:text-green-200 rounded-lg text-sm font-medium\">\n                Archivo seleccionado: {{ uploadedFile.name }}\n              </p>\n            </div>\n          </div>\n        </section>\n\n        <!-- Sección de contacto mejorada -->\n        <section class=\"mt-16 pb-12\">\n          <div class=\"bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <h3 class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent mb-2\">Contacto</h3>\n            <p class=\"text-gray-600 dark:text-gray-400 mb-6\">Si tienes preguntas o quieres un presupuesto personalizado, escríbenos.</p>\n            <form @submit.prevent=\"submitContact\" class=\"space-y-6 max-w-xl mx-auto\">\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Nombre</label>\n                <input \n                  v-model=\"contact.name\" \n                  type=\"text\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required \n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Email</label>\n                <input \n                  v-model=\"contact.email\" \n                  type=\"email\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required \n                />\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium pb-2 text-gray-700 dark:text-gray-300\">Mensaje</label>\n                <textarea \n                  v-model=\"contact.message\" \n                  rows=\"4\" \n                  class=\"block w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\" \n                  required\n                ></textarea>\n              </div>\n              <div class=\"flex justify-end\">\n                <button \n                  type=\"submit\" \n                  class=\"px-8 py-3 rounded-xl bg-gradient-to-r from-purple-500 to-blue-500 text-white font-medium shadow-md hover:shadow-lg transition-all duration-300 hover:from-purple-600 hover:to-blue-600 transform hover:-translate-y-0.5\"\n                >\n                  Enviar Mensaje\n                </button>\n              </div>\n            </form>\n          </div>\n        </section>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppHeader from '@/components/Header.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // muestras\n      sampleProducts: [\n        { id: 'p1', name: 'Camisa - Muestra', desc: 'Sube una foto de una camisa impresa' },\n        { id: 'p2', name: 'Taza - Muestra', desc: 'Sube una foto de una taza impresa' },\n        { id: 'p3', name: 'Termo - Muestra', desc: 'Sube una foto de un termo impreso' }\n      ],\n      samplesPreview: {},\n      samplesFiles: {},\n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    }\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click()\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0]\n      // Aquí puedes procesar el archivo subido\n      console.log('Archivo subido:', this.uploadedFile)\n    },\n    // Muestras (cards)\n    triggerSampleInput(id) {\n      const el = document.getElementById(`sampleFile_${id}`)\n      if (el) el.click()\n    },\n    handleSampleUpload(event, id) {\n      const file = event.target.files && event.target.files[0]\n      if (!file) return\n      // liberar preview anterior si existiera\n      if (this.samplesPreview[id]) {\n        try { URL.revokeObjectURL(this.samplesPreview[id]) } catch (e) { console.warn('Could not revoke object URL (sample):', e) }\n      }\n      this.samplesFiles[id] = file\n      this.samplesPreview[id] = URL.createObjectURL(file)\n      console.log('Muestra subida para', id, file)\n    },\n    // Diseño personalizado\n    handleDesignUpload(event) {\n      const file = event.target.files && event.target.files[0]\n      if (!file) return\n      if (this.designPreview) {\n        try { URL.revokeObjectURL(this.designPreview) } catch (e) { console.warn('Could not revoke object URL (design):', e) }\n      }\n      this.designFile = file\n      this.designPreview = URL.createObjectURL(file)\n      console.log('Diseño cargado:', file)\n    },\n    submitDesign() {\n      if (!this.designFile) {\n        alert('Por favor sube una imagen para tu diseño.')\n        return\n      }\n      if (!this.designProductType) {\n        alert('Selecciona el producto donde quieres imprimir el diseño.')\n        return\n      }\n      // En una app real enviaríamos a backend o al carrito. Aquí simulamos agregar 1 al carrito y redirigir.\n      this.cartCount += 1\n      console.log('Diseño enviado:', { file: this.designFile, product: this.designProductType, notes: this.designNotes })\n      // Limpieza simple\n      this.designFile = null\n      try { URL.revokeObjectURL(this.designPreview) } catch (e) { console.warn('Could not revoke object URL (cleanup):', e) }\n      this.designPreview = null\n      this.designProductType = ''\n      this.designNotes = ''\n      this.$router.push('/cart')\n    },\n    // Contacto\n    submitContact() {\n      // Aquí enviaríamos a un endpoint. Por ahora solo log y notificación.\n      console.log('Contacto enviado:', this.contact)\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contactaremos pronto.')\n      this.contact = { name: '', email: '', message: '' }\n    },\n    addToCart() {\n      // Lógica para agregar al carrito\n      this.cartCount += this.quantity\n      \n      // Redirigir al carrito\n      this.$router.push('/cart')\n    }\n  }\n}\n</script>"],"mappings":";;;;AA8JA,OAAOA,SAAQ,MAAO,yBAAwB;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE,CAAC;MACXC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,IAAI;MAClBC,SAAS,EAAE,CAAC;MACZ;MACAC,cAAc,EAAE,CACd;QAAEC,EAAE,EAAE,IAAI;QAAET,IAAI,EAAE,kBAAkB;QAAEU,IAAI,EAAE;MAAsC,CAAC,EACnF;QAAED,EAAE,EAAE,IAAI;QAAET,IAAI,EAAE,gBAAgB;QAAEU,IAAI,EAAE;MAAoC,CAAC,EAC/E;QAAED,EAAE,EAAE,IAAI;QAAET,IAAI,EAAE,iBAAiB;QAAEU,IAAI,EAAE;MAAoC,EAChF;MACDC,cAAc,EAAE,CAAC,CAAC;MAClBC,YAAY,EAAE,CAAC,CAAC;MAChB;MACAC,UAAU,EAAE,IAAI;MAChBC,aAAa,EAAE,IAAI;MACnBC,iBAAiB,EAAE,EAAE;MACrBC,WAAW,EAAE,EAAE;MACf;MACAC,OAAO,EAAE;QACPjB,IAAI,EAAE,EAAE;QACRkB,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjBC,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,KAAK,CAAC;IAC7C,CAAC;IACDC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACpB,YAAW,GAAIoB,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;MACxC;MACAC,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACxB,YAAY;IAClD,CAAC;IACD;IACAyB,kBAAkBA,CAACtB,EAAE,EAAE;MACrB,MAAMuB,EAAC,GAAIV,QAAQ,CAACC,cAAc,CAAC,cAAcd,EAAE,EAAE;MACrD,IAAIuB,EAAE,EAAEA,EAAE,CAACR,KAAK,CAAC;IACnB,CAAC;IACDS,kBAAkBA,CAACP,KAAK,EAAEjB,EAAE,EAAE;MAC5B,MAAMyB,IAAG,GAAIR,KAAK,CAACC,MAAM,CAACC,KAAI,IAAKF,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;MACvD,IAAI,CAACM,IAAI,EAAE;MACX;MACA,IAAI,IAAI,CAACvB,cAAc,CAACF,EAAE,CAAC,EAAE;QAC3B,IAAI;UAAE0B,GAAG,CAACC,eAAe,CAAC,IAAI,CAACzB,cAAc,CAACF,EAAE,CAAC;QAAE,EAAE,OAAO4B,CAAC,EAAE;UAAER,OAAO,CAACS,IAAI,CAAC,uCAAuC,EAAED,CAAC;QAAE;MAC5H;MACA,IAAI,CAACzB,YAAY,CAACH,EAAE,IAAIyB,IAAG;MAC3B,IAAI,CAACvB,cAAc,CAACF,EAAE,IAAI0B,GAAG,CAACI,eAAe,CAACL,IAAI;MAClDL,OAAO,CAACC,GAAG,CAAC,qB