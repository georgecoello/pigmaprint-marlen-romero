tion value=\"gorra\">Gorra</option>\n                  <option value=\"otros\">Productos Varios</option>\n                </select>\n              </div>\n              <div>\n                <label class=\"block text-sm font-medium pb-2\" for=\"quantity\">Cantidad</label>\n                <input \n                  v-model.number=\"quantity\"\n                  class=\"block w-full rounded-lg border-border-light dark:border-border-dark bg-background-light dark:bg-background-dark focus:border-primary focus:ring-primary text-base p-3 placeholder:text-foreground-light/50 dark:placeholder:text-foreground-dark/50\" \n                  id=\"quantity\" \n                  min=\"1\" \n                  type=\"number\"\n                  required\n                />\n              </div>\n            </div>\n            <div>\n              <label class=\"block text-sm font-medium pb-2\" for=\"instructions\">Instrucciones adicionales (opcional)</label>\n              <textarea \n                v-model=\"instructions\"\n                class=\"block w-full rounded-lg border-border-light dark:border-border-dark bg-background-light dark:bg-background-dark focus:border-primary focus:ring-primary text-base p-3 placeholder:text-foreground-light/50 dark:placeholder:text-foreground-dark/50\" \n                id=\"instructions\" \n                rows=\"4\"\n                placeholder=\"Especifica detalles adicionales sobre tu pedido, como colores, tallas, etc.\"\n              ></textarea>\n            </div>\n            <div class=\"flex justify-end pt-4\">\n              <button \n                type=\"submit\"\n                class=\"flex min-w-[150px] items-center justify-center gap-2 rounded-lg h-12 px-6 bg-primary text-white text-base font-bold leading-normal tracking-wide hover:bg-primary/90 transition-colors\"\n              >\n                <span class=\"material-symbols-outlined\">add_shopping_cart</span>\n                <span>Agregar al Carrito</span>\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppHeader from '@/components/Header.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0\n    }\n  },\n  methods: {\n    triggerFileInput() {\n      document.getElementById('fileInput').click()\n    },\n    handleFileUpload(event) {\n      this.uploadedFile = event.target.files[0]\n      // Aquí puedes procesar el archivo subido\n      console.log('Archivo subido:', this.uploadedFile)\n    },\n    addToCart() {\n      // Lógica para agregar al carrito\n      this.cartCount += this.quantity\n      \n      // Redirigir al carrito\n      this.$router.push('/cart')\n    }\n  }\n}\n</script>"],"mappings":";AA2FA,OAAOA,SAAQ,MAAO,yBAAwB;AAE9C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE,CAAC;MACXC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,IAAI;MAClBC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjBC,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,KAAK,CAAC;IAC7C,CAAC;IACDC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACR,YAAW,GAAIQ,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC;MACxC;MACAC,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACZ,YAAY;IAClD,CAAC;IACDa,SAASA,CAAA,EAAG;MACV;MACA,IAAI,CAACZ,SAAQ,IAAK,IAAI,CAACH,QAAO;;MAE9B;MACA,IAAI,CAACgB,OAAO,CAACC,IAAI,CAAC,OAAO;IAC3B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}