{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport AppHeader from '@/components/Header.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // Previews para las 15 cards\n      samplePreviews: {},\n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    };\n  },\n  computed: {\n    canSubmitDesign() {\n      return this.designProductType && this.uploadedFile;\n    }\n  },\n  methods: {\n    // Métodos para las cards de muestras\n    triggerFileInput(cardNumber) {\n      document.getElementById(`fileInput${cardNumber}`).click();\n    },\n    handleSampleUpload(event, cardNumber) {\n      const file = event.target.files[0];\n      if (!file) return;\n\n      // Validar tipo de archivo\n      if (!file.type.startsWith('image/')) {\n        alert('Por favor selecciona solo archivos de imagen.');\n        return;\n      }\n\n      // Validar tamaño (5MB máximo)\n      if (file.size > 5 * 1024 * 1024) {\n        alert('La imagen es demasiado grande. Máximo 5MB permitido.');\n        return;\n      }\n\n      // Crear URL temporal para previsualización\n      if (this.samplePreviews[cardNumber]) {\n        URL.revokeObjectURL(this.samplePreviews[cardNumber]);\n      }\n      this.samplePreviews[cardNumber] = URL.createObjectURL(file);\n      console.log(`Imagen cargada para card ${cardNumber}:`, file);\n    },\n    // Selección de tipo de producto\n    selectProductType(type) {\n      this.designProductType = type;\n    },\n    getProductTypeLabel(type) {\n      const labels = {\n        'camiseta': 'Camiseta',\n        'taza': 'Taza',\n        'termo': 'Termo',\n        'gorra': 'Gorra',\n        'vaso': 'Vaso Térmico',\n        'llavero': 'Llavero',\n        'otros': 'Otro Producto'\n      };\n      return labels[type] || type;\n    },\n    // File upload for design section\n    triggerMainFileInput() {\n      document.getElementById('mainFileInput').click();\n    },\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n\n      // Validar tipo de archivo\n      const allowedTypes = ['.png', '.jpg', '.jpeg', '.svg', '.pdf'];\n      const fileExtension = '.' + file.name.split('.').pop().toLowerCase();\n      if (!allowedTypes.includes(fileExtension)) {\n        alert('Por favor selecciona un archivo PNG, JPG, SVG o PDF.');\n        return;\n      }\n\n      // Validar tamaño (10MB máximo)\n      if (file.size > 10 * 1024 * 1024) {\n        alert('El archivo es demasiado grande. Máximo 10MB permitido.');\n        return;\n      }\n      this.uploadedFile = file;\n      this.designFile = file;\n      console.log('Archivo de diseño subido:', file);\n    },\n    // Envío del diseño\n    submitDesign() {\n      if (!this.canSubmitDesign) {\n        alert('Por favor selecciona un producto y sube tu diseño.');\n        return;\n      }\n\n      // Simular envío al carrito\n      this.cartCount += 1;\n      console.log('Diseño enviado:', {\n        product: this.designProductType,\n        file: this.designFile,\n        notes: this.designNotes\n      });\n\n      // Mostrar confirmación\n      alert(`¡Perfecto! Tu diseño para ${this.getProductTypeLabel(this.designProductType)} ha sido enviado para cotización. Te contactaremos pronto.`);\n\n      // Limpiar formulario\n      this.designProductType = '';\n      this.designNotes = '';\n      this.uploadedFile = null;\n      this.designFile = null;\n\n      // Limpiar input file\n      document.getElementById('mainFileInput').value = '';\n    },\n    // Contacto\n    submitContact() {\n      console.log('Contacto enviado:', this.contact);\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contactaremos pronto.');\n      this.contact = {\n        name: '',\n        email: '',\n        message: ''\n      };\n    },\n    addToCart() {\n      this.cartCount += this.quantity;\n      this.$router.push('/cart');\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","name","components","data","selectedProduct","quantity","instructions","uploadedFile","cartCount","samplePreviews","designFile","designPreview","designProductType","designNotes","contact","email","message","computed","canSubmitDesign","methods","triggerFileInput","cardNumber","document","getElementById","click","handleSampleUpload","event","file","target","files","type","startsWith","alert","size","URL","revokeObjectURL","createObjectURL","console","log","selectProductType","getProductTypeLabel","labels","triggerMainFileInput","handleFileUpload","allowedTypes","fileExtension","split","pop","toLowerCase","includes","submitDesign","product","notes","value","submitContact","addToCart","$router","push"],"sources":["/home/jorge/Escritorio/Jorge-PC/Marlen/pigmaprint/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen bg-gradient-to-br from-gray-50 to-gray-100 dark:from-gray-900 dark:to-gray-800\">\n    <AppHeader :cart-count=\"cartCount\" />\n    <main class=\"flex-grow container mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <div class=\"max-w-6xl mx-auto\">\n        <!-- Texto de bienvenida -->\n        <section id=\"inicio\" class=\"mb-12 text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8 transform transition-all duration-300 hover:shadow-xl\">\n          <h1 class=\"text-4xl sm:text-5xl font-extrabold tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">\n            ¡Bienvenido a Pigmaprint!\n          </h1>\n          <p class=\"mt-6 text-lg text-gray-600 dark:text-gray-300 leading-relaxed\">\n            Donde tu imaginación se convierte en realidad. Creemos que cada idea merece ser única, por eso personalizamos todo lo que puedas imaginar: desde prendas hasta tazas, termos, vasos, llaveros y mucho más.\n          </p>\n          <div class=\"mt-8 bg-gradient-to-r from-purple-50 to-blue-50 dark:from-gray-700 dark:to-gray-600 rounded-xl p-6\">\n            <h3 class=\"text-xl font-bold text-gray-800 dark:text-white mb-4\">Tecnologías de impresión:</h3>\n            <ul class=\"grid grid-cols-1 md:grid-cols-3 gap-4 text-left\">\n              <li class=\"flex items-start\">\n                <span class=\"text-purple-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>UVDTF</strong> — acabados duraderos con relieve</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-blue-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>DTF textil</strong> — ideal para cualquier color o textura</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-green-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>Sublimación</strong> — resultados vibrantes</span>\n              </li>\n            </ul>\n          </div>\n          <p class=\"mt-8 text-base text-gray-600 dark:text-gray-300 italic border-l-4 border-purple-500 pl-4 py-2\">\n            Nuestra misión es empoderarte para que transformes tus ideas en piezas únicas, llenas de estilo, emoción y personalidad.\n          </p>\n        </section>\n        \n        <!-- Muestras de Productos - 15 Cards para agregar imágenes -->\n        <section id=\"gallery\" class=\"mt-16\">\n          <div class=\"text-center mb-10\">\n            <h3 id=\"trabajos\" class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent\">Nuestros Trabajos Realizados</h3>\n            <p class=\"mt-2 text-gray-600 dark:text-gray-400 max-w-2xl mx-auto\">Galería de productos personalizados que hemos creado para nuestros clientes.</p>\n          </div>\n          \n          <!-- 15 Cards para agregar imágenes -->\n          <div class=\"mt-6 grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-6\">\n            <!-- Cards 1-15 (se mantienen igual) -->\n            <!-- ... -->\n          </div>\n        </section>\n\n        <!-- Carga tu Diseño mejorado con selector de producto -->\n        <section id=\"carga-diseno\" class=\"mt-16\">\n          <div id=\"diseño\" class=\"text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <h2 class=\"text-4xl font-black tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">Carga tu Diseño</h2>\n            <p class=\"mt-4 text-lg text-gray-600 dark:text-gray-300\">Sube tu diseño para personalizar tus productos. Aceptamos archivos PNG, JPG, SVG y PDF.</p>\n          </div>\n\n          <!-- Formulario de carga de diseño -->\n          <div class=\"mt-8 bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8\">\n            <!-- Selección de producto -->\n            <div class=\"mb-8\">\n              <h3 class=\"text-xl font-semibold text-gray-800 dark:text-white mb-4\">\n                <span class=\"material-symbols-outlined align-middle mr-2 text-purple-500\">category</span>\n                ¿En qué producto quieres aplicar tu diseño?\n              </h3>\n              <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4\">\n                <!-- Opción 1 -->\n                <div \n                  @click=\"selectProductType('camiseta')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'camiseta' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">checkroom</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Camiseta</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Algodón 100% premium</p>\n                  </div>\n                </div>\n\n                <!-- Opción 2 -->\n                <div \n                  @click=\"selectProductType('taza')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'taza' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">coffee</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Taza</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Cerámica de alta calidad</p>\n                  </div>\n                </div>\n\n                <!-- Opción 3 -->\n                <div \n                  @click=\"selectProductType('termo')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'termo' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">water_bottle</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Termo</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Acero inoxidable</p>\n                  </div>\n                </div>\n\n                <!-- Opción 4 -->\n                <div \n                  @click=\"selectProductType('gorra')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'gorra' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">sports_baseball</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Gorra</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Ajustable con malla</p>\n                  </div>\n                </div>\n\n                <!-- Opción 5 -->\n                <div \n                  @click=\"selectProductType('vaso')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'vaso' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">liquor</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Vaso Térmico</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Double wall insulation</p>\n                  </div>\n                </div>\n\n                <!-- Opción 6 -->\n                <div \n                  @click=\"selectProductType('llavero')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'llavero' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">key</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Hoodie</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Acrílico personalizado</p>\n                  </div>\n                </div>\n\n                <!-- Opción 7 - Otros -->\n                <div \n                  @click=\"selectProductType('otros')\"\n                  :class=\"[\n                    'border-2 rounded-xl p-4 cursor-pointer transition-all duration-300',\n                    designProductType === 'otros' \n                      ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                      : 'border-gray-300 dark:border-gray-600 hover:border-purple-400'\n                  ]\"\n                >\n                  <div class=\"text-center\">\n                    <div class=\"text-3xl text-purple-500 mb-2\">\n                      <span class=\"material-symbols-outlined\">category</span>\n                    </div>\n                    <h4 class=\"font-medium text-gray-800 dark:text-white\">Otro Producto</h4>\n                    <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">Especifique en notas</p>\n                  </div>\n                </div>\n              </div>\n\n              <!-- Producto seleccionado -->\n              <div v-if=\"designProductType\" class=\"mt-4 p-4 bg-green-50 dark:bg-green-900/20 rounded-lg\">\n                <p class=\"text-green-700 dark:text-green-300 font-medium\">\n                  <span class=\"material-symbols-outlined align-middle mr-2\">check_circle</span>\n                  Producto seleccionado: <strong>{{ getProductTypeLabel(designProductType) }}</strong>\n                </p>\n              </div>\n            </div>\n\n            <!-- Instrucciones adicionales -->\n            <div class=\"mb-8\">\n              <h3 class=\"text-xl font-semibold text-gray-800 dark:text-white mb-4\">\n                <span class=\"material-symbols-outlined align-middle mr-2 text-purple-500\">notes</span>\n                Instrucciones adicionales (opcional)\n              </h3>\n              <textarea \n                v-model=\"designNotes\"\n                rows=\"3\"\n                class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300 resize-none\"\n                placeholder=\"Ej: Quiero el diseño en la parte trasera de la camiseta, usar colores vibrantes, tamaño mediano...\"\n              ></textarea>\n            </div>\n\n            <!-- Área de carga de archivos mejorada -->\n            <div>\n              <h3 class=\"text-xl font-semibold text-gray-800 dark:text-white mb-4\">\n                <span class=\"material-symbols-outlined align-middle mr-2 text-purple-500\">cloud_upload</span>\n                Sube tu diseño\n              </h3>\n              <div \n                @click=\"triggerMainFileInput\"\n                class=\"flex flex-col items-center justify-center border-2 border-dashed border-gray-300 dark:border-gray-600 rounded-2xl h-56 cursor-pointer transition-all duration-300 bg-gradient-to-br from-gray-50 to-white dark:from-gray-800 dark:to-gray-700 hover:border-purple-400 hover:shadow-md\"\n              >\n                <input \n                  id=\"mainFileInput\"\n                  type=\"file\"\n                  class=\"hidden\"\n                  @change=\"handleFileUpload\"\n                  accept=\".png, .jpg, .jpeg, .svg, .pdf\"\n                />\n                \n                <div v-if=\"!uploadedFile\" class=\"text-center\">\n                  <div class=\"text-6xl text-purple-400 mb-4\">\n                    <span class=\"material-symbols-outlined\">cloud_upload</span>\n                  </div>\n                  <p class=\"text-lg text-gray-600 dark:text-gray-400 font-medium\">Haz clic para seleccionar un archivo</p>\n                  <p class=\"mt-2 text-sm text-gray-500 dark:text-gray-500\">o arrástralo aquí</p>\n                  <p class=\"text-xs text-gray-400 dark:text-gray-500 mt-4\">\n                    Formatos: PNG, JPG, SVG, PDF • Máx. 10MB\n                  </p>\n                </div>\n\n                <div v-else class=\"text-center\">\n                  <div class=\"text-5xl text-green-500 mb-3\">\n                    <span class=\"material-symbols-outlined\">check_circle</span>\n                  </div>\n                  <p class=\"text-lg text-gray-700 dark:text-gray-300 font-medium\">¡Archivo listo!</p>\n                  <p class=\"text-sm text-gray-600 dark:text-gray-400 mt-1\">{{ uploadedFile.name }}</p>\n                  <button \n                    @click.stop=\"triggerMainFileInput\"\n                    class=\"mt-3 text-purple-600 dark:text-purple-400 hover:text-purple-700 dark:hover:text-purple-300 text-sm font-medium transition-colors\"\n                  >\n                    Cambiar archivo\n                  </button>\n                </div>\n              </div>\n            </div>\n\n            <!-- Botón de envío -->\n            <div class=\"mt-8 text-center\">\n              <button \n                @click=\"submitDesign\"\n                :disabled=\"!canSubmitDesign\"\n                :class=\"[\n                  'px-8 py-4 rounded-xl font-semibold transition-all duration-300 transform hover:-translate-y-0.5',\n                  canSubmitDesign\n                    ? 'bg-gradient-to-r from-purple-500 to-blue-500 text-white shadow-lg hover:shadow-xl hover:from-purple-600 hover:to-blue-600'\n                    : 'bg-gray-300 dark:bg-gray-600 text-gray-500 dark:text-gray-400 cursor-not-allowed'\n                ]\"\n              >\n                <span class=\"material-symbols-outlined align-middle mr-2\">send</span>\n                Enviar Diseño para Cotización\n              </button>\n              \n              <p v-if=\"!canSubmitDesign\" class=\"mt-3 text-sm text-gray-500 dark:text-gray-400\">\n                * Completa todos los campos requeridos\n              </p>\n            </div>\n          </div>\n        </section>\n\n        <!-- Sección de contacto mejorada con redes sociales -->\n        <section id=\"contacto\" class=\"mt-16 pb-12\">\n          <!-- ... (se mantiene igual) ... -->\n        </section>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppHeader from '@/components/Header.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      \n      // Previews para las 15 cards\n      samplePreviews: {},\n      \n      // diseño personalizado\n      designFile: null,\n      designPreview: null,\n      designProductType: '',\n      designNotes: '',\n      \n      // contacto\n      contact: {\n        name: '',\n        email: '',\n        message: ''\n      }\n    }\n  },\n  computed: {\n    canSubmitDesign() {\n      return this.designProductType && this.uploadedFile;\n    }\n  },\n  methods: {\n    // Métodos para las cards de muestras\n    triggerFileInput(cardNumber) {\n      document.getElementById(`fileInput${cardNumber}`).click();\n    },\n    \n    handleSampleUpload(event, cardNumber) {\n      const file = event.target.files[0];\n      if (!file) return;\n      \n      // Validar tipo de archivo\n      if (!file.type.startsWith('image/')) {\n        alert('Por favor selecciona solo archivos de imagen.');\n        return;\n      }\n      \n      // Validar tamaño (5MB máximo)\n      if (file.size > 5 * 1024 * 1024) {\n        alert('La imagen es demasiado grande. Máximo 5MB permitido.');\n        return;\n      }\n      \n      // Crear URL temporal para previsualización\n      if (this.samplePreviews[cardNumber]) {\n        URL.revokeObjectURL(this.samplePreviews[cardNumber]);\n      }\n      \n      this.samplePreviews[cardNumber] = URL.createObjectURL(file);\n      console.log(`Imagen cargada para card ${cardNumber}:`, file);\n    },\n    \n    // Selección de tipo de producto\n    selectProductType(type) {\n      this.designProductType = type;\n    },\n    \n    getProductTypeLabel(type) {\n      const labels = {\n        'camiseta': 'Camiseta',\n        'taza': 'Taza',\n        'termo': 'Termo',\n        'gorra': 'Gorra',\n        'vaso': 'Vaso Térmico',\n        'llavero': 'Llavero',\n        'otros': 'Otro Producto'\n      };\n      return labels[type] || type;\n    },\n    \n    // File upload for design section\n    triggerMainFileInput() {\n      document.getElementById('mainFileInput').click();\n    },\n    \n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (!file) return;\n      \n      // Validar tipo de archivo\n      const allowedTypes = ['.png', '.jpg', '.jpeg', '.svg', '.pdf'];\n      const fileExtension = '.' + file.name.split('.').pop().toLowerCase();\n      \n      if (!allowedTypes.includes(fileExtension)) {\n        alert('Por favor selecciona un archivo PNG, JPG, SVG o PDF.');\n        return;\n      }\n      \n      // Validar tamaño (10MB máximo)\n      if (file.size > 10 * 1024 * 1024) {\n        alert('El archivo es demasiado grande. Máximo 10MB permitido.');\n        return;\n      }\n      \n      this.uploadedFile = file;\n      this.designFile = file;\n      console.log('Archivo de diseño subido:', file);\n    },\n    \n    // Envío del diseño\n    submitDesign() {\n      if (!this.canSubmitDesign) {\n        alert('Por favor selecciona un producto y sube tu diseño.');\n        return;\n      }\n      \n      // Simular envío al carrito\n      this.cartCount += 1;\n      \n      console.log('Diseño enviado:', {\n        product: this.designProductType,\n        file: this.designFile,\n        notes: this.designNotes\n      });\n      \n      // Mostrar confirmación\n      alert(`¡Perfecto! Tu diseño para ${this.getProductTypeLabel(this.designProductType)} ha sido enviado para cotización. Te contactaremos pronto.`);\n      \n      // Limpiar formulario\n      this.designProductType = '';\n      this.designNotes = '';\n      this.uploadedFile = null;\n      this.designFile = null;\n      \n      // Limpiar input file\n      document.getElementById('mainFileInput').value = '';\n    },\n    \n    // Contacto\n    submitContact() {\n      console.log('Contacto enviado:', this.contact);\n      alert('Gracias, ' + (this.contact.name || '') + '. Hemos recibido tu mensaje y te contacta