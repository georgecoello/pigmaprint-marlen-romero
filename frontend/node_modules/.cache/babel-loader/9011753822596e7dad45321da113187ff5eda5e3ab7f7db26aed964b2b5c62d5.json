{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport AppHeader from '@/components/Header.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      // Estado de modales\n      showUploadModal: false,\n      showLoginModal: false,\n      dragActive: false,\n      // Usuario actual\n      currentUser: null,\n      // Formulario de login\n      loginForm: {\n        email: '',\n        password: ''\n      },\n      // Usuarios del sistema (en producción esto vendría de una API)\n      users: [{\n        id: 1,\n        email: 'admin@pigmaprint.com',\n        password: 'admin123',\n        name: 'Administrador',\n        role: 'admin'\n      }, {\n        id: 2,\n        email: 'cliente@ejemplo.com',\n        password: 'cliente123',\n        name: 'Cliente Ejemplo',\n        role: 'user'\n      }],\n      // Muestras de productos existentes\n      sampleProducts: [{\n        id: 'p1',\n        name: 'Camiseta Personalizada',\n        desc: 'Diseño de logo empresarial en algodón 100%',\n        image: '/images/sample-tshirt.jpg',\n        category: 'ropa'\n      }, {\n        id: 'p2',\n        name: 'Taza Cerámica',\n        desc: 'Sublimación full color con diseño único',\n        image: '/images/sample-mug.jpg',\n        category: 'tazas'\n      }],\n      // Nueva muestra para administrador\n      newSample: {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null,\n        category: ''\n      }\n    };\n  },\n  computed: {\n    isAdmin() {\n      return this.currentUser && this.currentUser.role === 'admin';\n    },\n    canSubmit() {\n      return this.newSample.name && this.newSample.desc && this.newSample.image && this.newSample.category;\n    }\n  },\n  methods: {\n    // Sistema de autenticación\n    handleLogin() {\n      const user = this.users.find(u => u.email === this.loginForm.email && u.password === this.loginForm.password);\n      if (user) {\n        this.currentUser = {\n          ...user\n        };\n        this.showLoginModal = false;\n        this.loginForm = {\n          email: '',\n          password: ''\n        };\n        alert(`¡Bienvenido ${user.name}!`);\n      } else {\n        alert('Credenciales incorrectas. Por favor intenta de nuevo.');\n      }\n    },\n    handleLogout() {\n      this.currentUser = null;\n      alert('Sesión cerrada correctamente.');\n    },\n    getCategoryLabel(category) {\n      const labels = {\n        'ropa': 'Ropa',\n        'tazas': 'Tazas y Vasos',\n        'accesorios': 'Accesorios',\n        'hogar': 'Hogar',\n        'otros': 'Otros'\n      };\n      return labels[category] || 'Sin categoría';\n    },\n    // Modal methods\n    openModal() {\n      this.showUploadModal = true;\n    },\n    closeModal() {\n      this.showUploadModal = false;\n      this.resetForm();\n    },\n    resetForm() {\n      this.newSample = {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null,\n        category: ''\n      };\n      this.dragActive = false;\n    },\n    // File handling methods\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    handleFileSelect(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.processFile(file);\n      }\n    },\n    handleDrop(event) {\n      event.preventDefault();\n      this.dragActive = false;\n      const files = event.dataTransfer.files;\n      if (files.length > 0) {\n        this.processFile(files[0]);\n      }\n    },\n    handleDragOver(event) {\n      event.preventDefault();\n      this.dragActive = true;\n    },\n    processFile(file) {\n      if (!file.type.startsWith('image/')) {\n        alert('Por favor selecciona solo archivos de imagen.');\n        return;\n      }\n      if (file.size > 5 * 1024 * 1024) {\n        alert('La imagen es demasiado grande. Máximo 5MB permitido.');\n        return;\n      }\n      this.newSample.image = URL.createObjectURL(file);\n      this.newSample.imageFile = file;\n    },\n    removePreview() {\n      if (this.newSample.image) {\n        URL.revokeObjectURL(this.newSample.image);\n      }\n      this.newSample.image = null;\n      this.newSample.imageFile = null;\n      this.$refs.fileInput.value = '';\n    },\n    // Sample management\n    addNewSample() {\n      if (!this.canSubmit) {\n        alert('Por favor completa todos los campos requeridos.');\n        return;\n      }\n      const newProduct = {\n        id: 'p' + Date.now(),\n        name: this.newSample.name,\n        desc: this.newSample.desc,\n        image: this.newSample.image,\n        category: this.newSample.category\n      };\n      this.sampleProducts.unshift(newProduct);\n      this.closeModal();\n      alert('¡Muestra agregada exitosamente a la galería!');\n    },\n    removeSample(productId) {\n      if (confirm('¿Estás seguro de que quieres eliminar esta muestra?')) {\n        const index = this.sampleProducts.findIndex(p => p.id === productId);\n        if (index !== -1) {\n          const product = this.sampleProducts[index];\n          if (product.image && product.image.startsWith('blob:')) {\n            URL.revokeObjectURL(product.image);\n          }\n          this.sampleProducts.splice(index, 1);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["AppHeader","name","components","data","selectedProduct","quantity","instructions","uploadedFile","cartCount","showUploadModal","showLoginModal","dragActive","currentUser","loginForm","email","password","users","id","role","sampleProducts","desc","image","category","newSample","imageFile","computed","isAdmin","canSubmit","methods","handleLogin","user","find","u","alert","handleLogout","getCategoryLabel","labels","openModal","closeModal","resetForm","triggerFileInput","$refs","fileInput","click","handleFileSelect","event","file","target","files","processFile","handleDrop","preventDefault","dataTransfer","length","handleDragOver","type","startsWith","size","URL","createObjectURL","removePreview","revokeObjectURL","value","addNewSample","newProduct","Date","now","unshift","removeSample","productId","confirm","index","findIndex","p","product","splice"],"sources":["/home/jorge/Escritorio/Jorge-PC/Marlen/pigmaprint/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col min-h-screen bg-gradient-to-br from-gray-50 to-gray-100 dark:from-gray-900 dark:to-gray-800\">\n    <AppHeader :cart-count=\"cartCount\" :user=\"currentUser\" @login=\"showLoginModal = true\" @logout=\"handleLogout\" />\n    <main class=\"flex-grow container mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <div class=\"max-w-3xl mx-auto\">\n        <!-- Texto de bienvenida mejorado -->\n        <section class=\"mb-12 text-center bg-white dark:bg-gray-800 rounded-2xl shadow-lg p-8 transform transition-all duration-300 hover:shadow-xl\">\n          <h1 class=\"text-4xl sm:text-5xl font-extrabold tracking-tight bg-gradient-to-r from-purple-600 to-blue-500 bg-clip-text text-transparent\">\n            ¡Bienvenido a Pigmaprint!\n          </h1>\n          <p class=\"mt-6 text-lg text-gray-600 dark:text-gray-300 leading-relaxed\">\n            Donde tu imaginación se convierte en realidad. Creemos que cada idea merece ser única, por eso personalizamos todo lo que puedas imaginar: desde prendas hasta tazas, termos, vasos, llaveros y mucho más.\n          </p>\n          <div class=\"mt-8 bg-gradient-to-r from-purple-50 to-blue-50 dark:from-gray-700 dark:to-gray-600 rounded-xl p-6\">\n            <h3 class=\"text-xl font-bold text-gray-800 dark:text-white mb-4\">Tecnologías de impresión:</h3>\n            <ul class=\"grid grid-cols-1 md:grid-cols-3 gap-4 text-left\">\n              <li class=\"flex items-start\">\n                <span class=\"text-purple-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>UVDTF</strong> — acabados duraderos con relieve</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-blue-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>DTF textil</strong> — ideal para cualquier color o textura</span>\n              </li>\n              <li class=\"flex items-start\">\n                <span class=\"text-green-500 mr-2 mt-1\">•</span>\n                <span class=\"text-gray-700 dark:text-gray-300\"><strong>Sublimación</strong> — resultados vibrantes</span>\n              </li>\n            </ul>\n          </div>\n          <p class=\"mt-8 text-base text-gray-600 dark:text-gray-300 italic border-l-4 border-purple-500 pl-4 py-2\">\n            Nuestra misión es empoderarte para que transformes tus ideas en piezas únicas, llenas de estilo, emoción y personalidad.\n          </p>\n        </section>\n        \n        <!-- Muestras de Productos - Ahora para admin subir productos creados -->\n        <section class=\"mt-16\">\n          <div class=\"text-center mb-10\">\n            <h3 class=\"text-3xl font-bold bg-gradient-to-r from-gray-800 to-gray-600 dark:from-white dark:to-gray-300 bg-clip-text text-transparent\">Nuestros Trabajos Realizados</h3>\n            <p class=\"mt-2 text-gray-600 dark:text-gray-400 max-w-2xl mx-auto\">Galería de productos personalizados que hemos creado para nuestros clientes.</p>\n            \n            <!-- Botón para abrir modal de administrador (solo visible para admin) -->\n            <div v-if=\"isAdmin\" class=\"mt-6\">\n              <button \n                @click=\"showUploadModal = true\"\n                class=\"inline-flex items-center gap-2 px-6 py-3 bg-gradient-to-r from-purple-500 to-blue-500 hover:from-purple-600 hover:to-blue-600 text-white font-semibold rounded-xl shadow-md hover:shadow-lg transition-all duration-300 transform hover:-translate-y-0.5\"\n              >\n                <span class=\"material-symbols-outlined\">add_photo_alternate</span>\n                Agregar Nueva Muestra\n              </button>\n            </div>\n\n            <!-- Botón de login para usuarios normales -->\n            <div v-else-if=\"!currentUser\" class=\"mt-6\">\n              <button \n                @click=\"showLoginModal = true\"\n                class=\"inline-flex items-center gap-2 px-6 py-3 border border-purple-500 text-purple-500 hover:bg-purple-500 hover:text-white font-semibold rounded-xl transition-all duration-300\"\n              >\n                <span class=\"material-symbols-outlined\">login</span>\n                Iniciar Sesión\n              </button>\n            </div>\n          </div>\n          \n          <!-- Galería de muestras existentes -->\n          <div class=\"mt-6 grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8\">\n            <div \n              v-for=\"product in sampleProducts\" \n              :key=\"product.id\" \n              class=\"border border-gray-200 dark:border-gray-700 rounded-xl p-6 bg-white dark:bg-gray-800 shadow-md hover:shadow-lg transition-all duration-300 transform hover:-translate-y-1\"\n            >\n              <div class=\"relative\">\n                <img \n                  :src=\"product.image\" \n                  :alt=\"product.name\" \n                  class=\"w-full h-48 object-cover rounded-lg mb-4\"\n                />\n                <span \n                  v-if=\"isAdmin\"\n                  @click=\"removeSample(product.id)\"\n                  class=\"absolute top-2 right-2 bg-red-500 text-white p-2 rounded-full cursor-pointer hover:bg-red-600 transition-colors shadow-lg\"\n                  title=\"Eliminar muestra\"\n                >\n                  <span class=\"material-symbols-outlined text-sm\">delete</span>\n                </span>\n              </div>\n              <h4 class=\"font-bold text-lg text-gray-800 dark:text-white\">{{ product.name }}</h4>\n              <p class=\"mt-2 text-sm text-gray-600 dark:text-gray-400\">{{ product.desc }}</p>\n              <div class=\"mt-3\">\n                <span class=\"inline-block px-3 py-1 bg-purple-100 dark:bg-purple-900 text-purple-800 dark:text-purple-200 rounded-full text-xs font-medium\">\n                  {{ getCategoryLabel(product.category) }}\n                </span>\n              </div>\n            </div>\n          </div>\n          \n          <!-- Mensaje cuando no hay muestras -->\n          <div v-if=\"sampleProducts.length === 0\" class=\"text-center py-12\">\n            <div class=\"text-6xl text-gray-300 dark:text-gray-600 mb-4\">\n              <span class=\"material-symbols-outlined\">photo_library</span>\n            </div>\n            <p class=\"text-gray-500 dark:text-gray-400\">Aún no hay muestras en la galería.</p>\n            <p v-if=\"isAdmin\" class=\"text-sm text-gray-400 dark:text-gray-500 mt-2\">\n              Haz clic en \"Agregar Nueva Muestra\" para comenzar.\n            </p>\n          </div>\n        </section>\n\n        <!-- Modal para subir nuevas muestras (solo para admin) -->\n        <div v-if=\"showUploadModal && isAdmin\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center p-4 z-50\">\n          <div class=\"bg-white dark:bg-gray-800 rounded-2xl shadow-2xl w-full max-w-2xl max-h-[90vh] overflow-hidden transform transition-all duration-300 scale-100\">\n            <!-- Header del Modal -->\n            <div class=\"bg-gradient-to-r from-purple-500 to-blue-500 p-6\">\n              <div class=\"flex items-center justify-between\">\n                <h3 class=\"text-xl font-bold text-white\">\n                  <span class=\"material-symbols-outlined align-middle mr-2\">add_photo_alternate</span>\n                  Agregar Nueva Muestra\n                </h3>\n                <button \n                  @click=\"closeModal\"\n                  class=\"text-white hover:bg-white hover:bg-opacity-20 p-2 rounded-full transition-colors duration-200\"\n                >\n                  <span class=\"material-symbols-outlined\">close</span>\n                </button>\n              </div>\n            </div>\n\n            <!-- Contenido del Modal -->\n            <div class=\"p-6 overflow-y-auto max-h-[calc(90vh-120px)]\">\n              <!-- Área de arrastrar y soltar -->\n              <div \n                @drop=\"handleDrop\"\n                @dragover=\"handleDragOver\"\n                @dragleave=\"dragActive = false\"\n                :class=\"[\n                  'border-2 border-dashed rounded-xl p-8 text-center cursor-pointer transition-all duration-300 mb-6',\n                  dragActive \n                    ? 'border-purple-500 bg-purple-50 dark:bg-purple-900/20' \n                    : 'border-gray-300 dark:border-gray-600 bg-gray-50 dark:bg-gray-700/50 hover:border-purple-400'\n                ]\"\n              >\n                <input \n                  id=\"modalFileInput\"\n                  ref=\"fileInput\"\n                  type=\"file\"\n                  class=\"hidden\"\n                  accept=\"image/*\"\n                  @change=\"handleFileSelect\"\n                />\n                \n                <div v-if=\"!newSample.image\" class=\"space-y-4\">\n                  <div class=\"text-5xl text-purple-400 mb-4\">\n                    <span class=\"material-symbols-outlined\">cloud_upload</span>\n                  </div>\n                  <h4 class=\"text-lg font-semibold text-gray-700 dark:text-gray-300\">\n                    Arrastra tu imagen aquí\n                  </h4>\n                  <p class=\"text-gray-500 dark:text-gray-400 text-sm\">\n                    o haz clic para seleccionar un archivo\n                  </p>\n                  <p class=\"text-xs text-gray-400 dark:text-gray-500 mt-2\">\n                    Formatos soportados: JPG, PNG, WebP • Máx. 5MB\n                  </p>\n                  <button \n                    @click=\"triggerFileInput\"\n                    class=\"mt-4 px-6 py-2 bg-purple-500 hover:bg-purple-600 text-white rounded-lg transition-colors duration-300 font-medium\"\n                  >\n                    <span class=\"material-symbols-outlined align-middle mr-1\">upload</span>\n                    Seleccionar Imagen\n                  </button>\n                </div>\n\n                <!-- Vista previa de la imagen -->\n                <div v-else class=\"space-y-4\">\n                  <div class=\"relative inline-block\">\n                    <img \n                      :src=\"newSample.image\" \n                      alt=\"Vista previa\" \n                      class=\"w-48 h-48 object-cover rounded-lg shadow-md mx-auto\"\n                    />\n                    <button \n                      @click.stop=\"removePreview\"\n                      class=\"absolute -top-2 -right-2 bg-red-500 text-white p-1 rounded-full hover:bg-red-600 transition-colors shadow-lg\"\n                    >\n                      <span class=\"material-symbols-outlined text-sm\">close</span>\n                    </button>\n                  </div>\n                  <p class=\"text-green-600 dark:text-green-400 font-medium\">\n                    <span class=\"material-symbols-outlined align-middle mr-1\">check_circle</span>\n                    Imagen cargada correctamente\n                  </p>\n                  <button \n                    @click.stop=\"triggerFileInput\"\n                    class=\"text-purple-600 dark:text-purple-400 hover:text-purple-700 dark:hover:text-purple-300 text-sm font-medium transition-colors\"\n                  >\n                    Cambiar imagen\n                  </button>\n                </div>\n              </div>\n\n              <!-- Información de la muestra -->\n              <div class=\"space-y-4\">\n                <div>\n                  <label class=\"block text-sm font-semibold text-gray-700 dark:text-gray-300 mb-2\">\n                    <span class=\"material-symbols-outlined align-middle mr-1 text-purple-500\">title</span>\n                    Nombre del producto *\n                  </label>\n                  <input \n                    v-model=\"newSample.name\" \n                    type=\"text\" \n                    class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\"\n                    placeholder=\"Ej: Camiseta personalizada con logo\"\n                    required\n                  />\n                </div>\n\n                <div>\n                  <label class=\"block text-sm font-semibold text-gray-700 dark:text-gray-300 mb-2\">\n                    <span class=\"material-symbols-outlined align-middle mr-1 text-purple-500\">description</span>\n                    Descripción *\n                  </label>\n                  <textarea \n                    v-model=\"newSample.desc\" \n                    rows=\"3\"\n                    class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300 resize-none\"\n                    placeholder=\"Ej: Diseño de logo empresarial en algodón 100% premium...\"\n                    required\n                  ></textarea>\n                </div>\n\n                <div>\n                  <label class=\"block text-sm font-semibold text-gray-700 dark:text-gray-300 mb-2\">\n                    <span class=\"material-symbols-outlined align-middle mr-1 text-purple-500\">category</span>\n                    Categoría *\n                  </label>\n                  <select \n                    v-model=\"newSample.category\"\n                    class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent transition-all duration-300\"\n                    required\n                  >\n                    <option value=\"\">Selecciona una categoría</option>\n                    <option value=\"ropa\">Ropa</option>\n                    <option value=\"tazas\">Tazas y Vasos</option>\n                    <option value=\"accesorios\">Accesorios</option>\n                    <option value=\"hogar\">Hogar</option>\n                    <option value=\"otros\">Otros</option>\n                  </select>\n                </div>\n              </div>\n            </div>\n\n            <!-- Footer del Modal -->\n            <div class=\"border-t border-gray-200 dark:border-gray-700 p-6 bg-gray-50 dark:bg-gray-700/50\">\n              <div class=\"flex justify-end gap-3\">\n                <button \n                  @click=\"closeModal\"\n                  class=\"px-6 py-3 rounded-xl border border-gray-300 dark:border-gray-600 text-gray-700 dark:text-gray-300 hover:bg-gray-100 dark:hover:bg-gray-600 transition-colors duration-300 font-medium\"\n                >\n                  Cancelar\n                </button>\n                <button \n                  @click=\"addNewSample\"\n                  :disabled=\"!canSubmit\"\n                  :class=\"[\n                    'px-6 py-3 rounded-xl font-medium transition-all duration-300 flex items-center gap-2',\n                    canSubmit\n                      ? 'bg-gradient-to-r from-purple-500 to-blue-500 text-white shadow-md hover:shadow-lg transform hover:-translate-y-0.5'\n                      : 'bg-gray-300 dark:bg-gray-600 text-gray-500 dark:text-gray-400 cursor-not-allowed'\n                  ]\"\n                >\n                  <span class=\"material-symbols-outlined\">save</span>\n                  Guardar Muestra\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- Modal de Login -->\n        <div v-if=\"showLoginModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center p-4 z-50\">\n          <div class=\"bg-white dark:bg-gray-800 rounded-2xl shadow-2xl w-full max-w-md transform transition-all duration-300 scale-100\">\n            <div class=\"bg-gradient-to-r from-purple-500 to-blue-500 p-6 rounded-t-2xl\">\n              <div class=\"flex items-center justify-between\">\n                <h3 class=\"text-xl font-bold text-white\">\n                  <span class=\"material-symbols-outlined align-middle mr-2\">login</span>\n                  Iniciar Sesión\n                </h3>\n                <button \n                  @click=\"showLoginModal = false\"\n                  class=\"text-white hover:bg-white hover:bg-opacity-20 p-2 rounded-full transition-colors duration-200\"\n                >\n                  <span class=\"material-symbols-outlined\">close</span>\n                </button>\n              </div>\n            </div>\n\n            <div class=\"p-6\">\n              <form @submit.prevent=\"handleLogin\" class=\"space-y-4\">\n                <div>\n                  <label class=\"block text-sm font-semibold text-gray-700 dark:text-gray-300 mb-2\">Email</label>\n                  <input \n                    v-model=\"loginForm.email\"\n                    type=\"email\"\n                    class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent\"\n                    placeholder=\"admin@pigmaprint.com\"\n                    required\n                  />\n                </div>\n                <div>\n                  <label class=\"block text-sm font-semibold text-gray-700 dark:text-gray-300 mb-2\">Contraseña</label>\n                  <input \n                    v-model=\"loginForm.password\"\n                    type=\"password\"\n                    class=\"w-full rounded-xl p-4 border border-gray-300 dark:border-gray-600 bg-white dark:bg-gray-700 text-gray-800 dark:text-white focus:ring-2 focus:ring-purple-500 focus:border-transparent\"\n                    placeholder=\"••••••••\"\n                    required\n                  />\n                </div>\n                <button \n                  type=\"submit\"\n                  class=\"w-full py-3 bg-gradient-to-r from-purple-500 to-blue-500 text-white rounded-xl font-semibold hover:from-purple-600 hover:to-blue-600 transition-all duration-300\"\n                >\n                  Iniciar Sesión\n                </button>\n              </form>\n\n              <!-- Credenciales de ejemplo -->\n              <div class=\"mt-6 p-4 bg-yellow-50 dark:bg-yellow-900/20 rounded-lg\">\n                <p class=\"text-sm text-yellow-800 dark:text-yellow-200 text-center\">\n                  <strong>Demo:</strong> admin@pigmaprint.com / admin123\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- Resto del código (Carga de diseño y contacto) -->\n        <!-- ... -->\n        \n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport AppHeader from '@/components/Header.vue'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    AppHeader\n  },\n  data() {\n    return {\n      selectedProduct: '',\n      quantity: 1,\n      instructions: '',\n      uploadedFile: null,\n      cartCount: 0,\n      \n      // Estado de modales\n      showUploadModal: false,\n      showLoginModal: false,\n      dragActive: false,\n      \n      // Usuario actual\n      currentUser: null,\n      \n      // Formulario de login\n      loginForm: {\n        email: '',\n        password: ''\n      },\n      \n      // Usuarios del sistema (en producción esto vendría de una API)\n      users: [\n        {\n          id: 1,\n          email: 'admin@pigmaprint.com',\n          password: 'admin123',\n          name: 'Administrador',\n          role: 'admin'\n        },\n        {\n          id: 2,\n          email: 'cliente@ejemplo.com',\n          password: 'cliente123',\n          name: 'Cliente Ejemplo',\n          role: 'user'\n        }\n      ],\n      \n      // Muestras de productos existentes\n      sampleProducts: [\n        { \n          id: 'p1', \n          name: 'Camiseta Personalizada', \n          desc: 'Diseño de logo empresarial en algodón 100%',\n          image: '/images/sample-tshirt.jpg',\n          category: 'ropa'\n        },\n        { \n          id: 'p2', \n          name: 'Taza Cerámica', \n          desc: 'Sublimación full color con diseño único',\n          image: '/images/sample-mug.jpg',\n          category: 'tazas'\n        }\n      ],\n      \n      // Nueva muestra para administrador\n      newSample: {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null,\n        category: ''\n      }\n    }\n  },\n  computed: {\n    isAdmin() {\n      return this.currentUser && this.currentUser.role === 'admin';\n    },\n    canSubmit() {\n      return this.newSample.name && \n             this.newSample.desc && \n             this.newSample.image && \n             this.newSample.category;\n    }\n  },\n  methods: {\n    // Sistema de autenticación\n    handleLogin() {\n      const user = this.users.find(u => \n        u.email === this.loginForm.email && \n        u.password === this.loginForm.password\n      );\n      \n      if (user) {\n        this.currentUser = { ...user };\n        this.showLoginModal = false;\n        this.loginForm = { email: '', password: '' };\n        alert(`¡Bienvenido ${user.name}!`);\n      } else {\n        alert('Credenciales incorrectas. Por favor intenta de nuevo.');\n      }\n    },\n    \n    handleLogout() {\n      this.currentUser = null;\n      alert('Sesión cerrada correctamente.');\n    },\n    \n    getCategoryLabel(category) {\n      const labels = {\n        'ropa': 'Ropa',\n        'tazas': 'Tazas y Vasos',\n        'accesorios': 'Accesorios',\n        'hogar': 'Hogar',\n        'otros': 'Otros'\n      };\n      return labels[category] || 'Sin categoría';\n    },\n\n    // Modal methods\n    openModal() {\n      this.showUploadModal = true;\n    },\n    \n    closeModal() {\n      this.showUploadModal = false;\n      this.resetForm();\n    },\n    \n    resetForm() {\n      this.newSample = {\n        name: '',\n        desc: '',\n        image: null,\n        imageFile: null,\n        category: ''\n      };\n      this.dragActive = false;\n    },\n    \n    // File handling methods\n    triggerFileInput() {\n      this.$refs.fileInput.click();\n    },\n    \n    handleFileSelect(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.processFile(file);\n      }\n    },\n    \n    handleDrop(event) {\n      event.preventDefault();\n      this.dragActive = false;\n      \n      const files = event.dataTransfer.files;\n      if (files.length > 0) {\n        this.processFile(files[0]);\n      }\n    },\n    \n    handleDragOver(event) {\n      event.preventDefault();\n      this.dragActive = true;\n    },\n    \n    processFile(file) {\n      if (!file.type.startsWith('image/')) {\n        alert('Por favor selecciona solo archivos de imagen.');\n        return;\n      }\n      \n      if (file.size > 5 * 1024 * 1024) {\n        alert('La imagen es demasiado grande. Máximo 5MB permitido.');\n        return;\n      }\n      \n      this.newSample.image = URL.createObjectURL(file);\n      this.newSample.imageFile = file;\n    },\n    \n   